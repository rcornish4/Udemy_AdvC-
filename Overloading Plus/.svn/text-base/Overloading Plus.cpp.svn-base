#include <iostream>
#include "Complex.h"

int main( int argc, char** argv ) {
	caveofprogramming::Complex c1(3, 4);
	caveofprogramming::Complex c2(2, 3);

	caveofprogramming::Complex c3 = c1 + c2;

	std::cout << c1 << std::endl;

	std::cout << c1 + c2 + c3 << std::endl;

	caveofprogramming::Complex c4(4, 2);
	caveofprogramming::Complex c5 = c4 + 7;

	std::cout << c5 << std::endl;

	return 0;
}


