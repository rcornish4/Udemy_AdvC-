#include <iostream>
#include "Complex.h"

const std::ostream &operator<<( std::ostream &out, const caveofprogramming::Complex &test ) {
	out << "Real: " << test.getReal() << ", Imaginary: " << test.getImaginary() << std::flush;
	return out;
}

int main( int argc, char** argv ) {
	caveofprogramming::Complex complex1 = caveofprogramming::Complex(10.0, 10.5);

	std::cout << complex1 << std::endl;

	return 0;
}


